<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1920 1080" width="1920" height="1080">
  <!-- Background gradients -->
  <defs>
    <!-- Sky gradient with sunrise colors -->
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#FFE4B5;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#FFA07A;stop-opacity:1" />
      <stop offset="60%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4682B4;stop-opacity:1" />
    </linearGradient>
    
    <!-- Mountain layers -->
    <linearGradient id="mountainFar" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#6B8E23;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#2F4F2F;stop-opacity:0.9" />
    </linearGradient>
    
    <linearGradient id="mountainMid" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#228B22;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#006400;stop-opacity:1" />
    </linearGradient>
    
    <linearGradient id="mountainNear" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#32CD32;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#228B22;stop-opacity:1" />
    </linearGradient>
    
    <!-- Homestay wood texture -->
    <linearGradient id="woodGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#D2691E;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#8B4513;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#654321;stop-opacity:1" />
    </linearGradient>
    
    <!-- Roof gradient -->
    <linearGradient id="roofGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#8B0000;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4B0000;stop-opacity:1" />
    </linearGradient>
    
    <!-- Cloud gradient -->
    <radialGradient id="cloudGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#FFFFFF;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#F0F8FF;stop-opacity:0.6" />
    </radialGradient>
    
    <!-- Mist gradient -->
    <linearGradient id="mistGradient" x1="0%" y1="100%" x2="0%" y2="0%">
      <stop offset="0%" style="stop-color:#FFFFFF;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#F0F8FF;stop-opacity:0.4" />
      <stop offset="100%" style="stop-color:#FFFFFF;stop-opacity:0.1" />
    </linearGradient>
    
    <!-- Sun glow -->
    <radialGradient id="sunGlow" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#FFD700;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#FFA500;stop-opacity:0.4" />
      <stop offset="100%" style="stop-color:#FF8C00;stop-opacity:0.1" />
    </radialGradient>
  </defs>
  
  <!-- Sky background -->
  <rect width="1920" height="1080" fill="url(#skyGradient)"/>
  
  <!-- Sun -->
  <circle cx="1600" cy="200" r="80" fill="url(#sunGlow)"/>
  <circle cx="1600" cy="200" r="50" fill="#FFD700"/>
  
  <!-- Far mountains -->
  <path d="M0,600 Q480,400 960,500 Q1440,350 1920,450 L1920,1080 L0,1080 Z" fill="url(#mountainFar)"/>
  
  <!-- Mid mountains -->
  <path d="M0,700 Q320,500 640,600 Q960,450 1280,550 Q1600,400 1920,500 L1920,1080 L0,1080 Z" fill="url(#mountainMid)"/>
  
  <!-- Near mountains -->
  <path d="M0,800 Q240,650 480,750 Q720,600 960,700 Q1200,550 1440,650 Q1680,500 1920,600 L1920,1080 L0,1080 Z" fill="url(#mountainNear)"/>
  
  <!-- Sea of clouds -->
  <g opacity="0.7">
    <!-- Large cloud formations -->
    <ellipse cx="300" cy="850" rx="200" ry="80" fill="url(#cloudGradient)"/>
    <ellipse cx="500" cy="820" rx="150" ry="60" fill="url(#cloudGradient)"/>
    <ellipse cx="800" cy="880" rx="180" ry="70" fill="url(#cloudGradient)"/>
    <ellipse cx="1100" cy="840" rx="220" ry="90" fill="url(#cloudGradient)"/>
    <ellipse cx="1400" cy="870" rx="160" ry="65" fill="url(#cloudGradient)"/>
    <ellipse cx="1650" cy="820" rx="140" ry="55" fill="url(#cloudGradient)"/>
    
    <!-- Smaller cloud wisps -->
    <ellipse cx="200" cy="900" rx="80" ry="30" fill="url(#cloudGradient)"/>
    <ellipse cx="600" cy="780" rx="100" ry="40" fill="url(#cloudGradient)"/>
    <ellipse cx="900" cy="920" rx="90" ry="35" fill="url(#cloudGradient)"/>
    <ellipse cx="1300" cy="790" rx="70" ry="25" fill="url(#cloudGradient)"/>
    <ellipse cx="1750" cy="880" rx="85" ry="32" fill="url(#cloudGradient)"/>
  </g>
  
  <!-- Mist layers -->
  <rect x="0" y="700" width="1920" height="200" fill="url(#mistGradient)" opacity="0.3"/>
  
  <!-- Pine trees on ridges -->
  <g opacity="0.8">
    <!-- Left ridge trees -->
    <g transform="translate(150,650)">
      <polygon points="0,100 -15,150 15,150" fill="#0F4F0F"/>
      <polygon points="0,80 -12,130 12,130" fill="#1F5F1F"/>
      <polygon points="0,60 -10,110 10,110" fill="#2F6F2F"/>
      <rect x="-3" y="150" width="6" height="30" fill="#8B4513"/>
    </g>
    
    <g transform="translate(250,680)">
      <polygon points="0,80 -12,120 12,120" fill="#0F4F0F"/>
      <polygon points="0,65 -10,105 10,105" fill="#1F5F1F"/>
      <polygon points="0,50 -8,90 8,90" fill="#2F6F2F"/>
      <rect x="-2" y="120" width="4" height="25" fill="#8B4513"/>
    </g>
    
    <!-- Right ridge trees -->
    <g transform="translate(1650,620)">
      <polygon points="0,120 -18,180 18,180" fill="#0F4F0F"/>
      <polygon points="0,95 -15,155 15,155" fill="#1F5F1F"/>
      <polygon points="0,70 -12,130 12,130" fill="#2F6F2F"/>
      <rect x="-4" y="180" width="8" height="35" fill="#8B4513"/>
    </g>
    
    <g transform="translate(1750,660)">
      <polygon points="0,90 -14,140 14,140" fill="#0F4F0F"/>
      <polygon points="0,70 -11,120 11,120" fill="#1F5F1F"/>
      <polygon points="0,50 -9,100 9,100" fill="#2F6F2F"/>
      <rect x="-3" y="140" width="6" height="28" fill="#8B4513"/>
    </g>
  </g>
  
  <!-- Wooden homestay on the peak -->
  <g transform="translate(960,500)">
    <!-- Main house structure -->
    <rect x="-80" y="20" width="160" height="100" fill="url(#woodGradient)" rx="5"/>
    
    <!-- Roof -->
    <polygon points="-100,20 0,-20 100,20" fill="url(#roofGradient)"/>
    
    <!-- Roof details -->
    <polygon points="-95,22 -5,-18 5,-18 95,22" fill="#A0522D" opacity="0.7"/>
    
    <!-- Windows -->
    <rect x="-60" y="40" width="25" height="30" fill="#87CEEB" rx="3"/>
    <rect x="-25" y="40" width="25" height="30" fill="#87CEEB" rx="3"/>
    <rect x="10" y="40" width="25" height="30" fill="#87CEEB" rx="3"/>
    <rect x="45" y="40" width="25" height="30" fill="#87CEEB" rx="3"/>
    
    <!-- Window frames -->
    <rect x="-62" y="38" width="29" height="34" fill="none" stroke="#654321" stroke-width="2" rx="3"/>
    <rect x="-27" y="38" width="29" height="34" fill="none" stroke="#654321" stroke-width="2" rx="3"/>
    <rect x="8" y="38" width="29" height="34" fill="none" stroke="#654321" stroke-width="2" rx="3"/>
    <rect x="43" y="38" width="29" height="34" fill="none" stroke="#654321" stroke-width="2" rx="3"/>
    
    <!-- Door -->
    <rect x="-10" y="80" width="20" height="40" fill="#8B4513" rx="2"/>
    <circle cx="5" cy="100" r="2" fill="#FFD700"/>
    
    <!-- Balcony -->
    <rect x="-90" y="115" width="180" height="8" fill="#D2691E"/>
    <rect x="-85" y="123" width="5" height="15" fill="#8B4513"/>
    <rect x="-65" y="123" width="5" height="15" fill="#8B4513"/>
    <rect x="-45" y="123" width="5" height="15" fill="#8B4513"/>
    <rect x="-25" y="123" width="5" height="15" fill="#8B4513"/>
    <rect x="-5" y="123" width="5" height="15" fill="#8B4513"/>
    <rect x="15" y="123" width="5" height="15" fill="#8B4513"/>
    <rect x="35" y="123" width="5" height="15" fill="#8B4513"/>
    <rect x="55" y="123" width="5" height="15" fill="#8B4513"/>
    <rect x="75" y="123" width="5" height="15" fill="#8B4513"/>
    
    <!-- Chimney -->
    <rect x="60" y="-10" width="15" height="40" fill="#8B4513"/>
    <rect x="58" y="-12" width="19" height="8" fill="#654321"/>
    
    <!-- Smoke -->
    <g opacity="0.6">
      <ellipse cx="67" cy="-20" rx="8" ry="15" fill="#F5F5F5"/>
      <ellipse cx="70" cy="-35" rx="6" ry="12" fill="#F5F5F5"/>
      <ellipse cx="65" cy="-50" rx="4" ry="8" fill="#F5F5F5"/>
    </g>
  </g>
  
  <!-- Additional atmospheric elements -->
  <!-- Light rays from sun -->
  <g opacity="0.3">
    <polygon points="1600,200 1500,400 1520,400 1600,220" fill="#FFD700"/>
    <polygon points="1600,200 1480,450 1500,450 1600,220" fill="#FFD700"/>
    <polygon points="1600,200 1460,500 1480,500 1600,220" fill="#FFD700"/>
  </g>
  
  <!-- Atmospheric haze -->
  <rect x="0" y="0" width="1920" height="1080" fill="url(#mistGradient)" opacity="0.1"/>
  
  <!-- Vignette effect -->
  <defs>
    <radialGradient id="vignette" cx="50%" cy="50%" r="70%">
      <stop offset="0%" style="stop-color:#000000;stop-opacity:0" />
      <stop offset="100%" style="stop-color:#000000;stop-opacity:0.3" />
    </radialGradient>
  </defs>
  <rect x="0" y="0" width="1920" height="1080" fill="url(#vignette)"/>
</svg>